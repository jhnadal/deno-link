{
  "org": "jhnadal",
  "app": "nadal-link-shortener",
  "tasks": {
    "dev": "deno run --watch -A --unstable-kv --env src/main.ts --tunnel",
    "test": "deno test tests/",
    "deploy": "deno deploy . --prod"
  },
  "imports": {
    "@deno/kv-oauth": "jsr:@deno/kv-oauth@^0.11.0",
    "@std/assert": "jsr:@std/assert@1",
    "@std/async": "jsr:@std/async@^1.0.15",
    "@std/collections": "jsr:@std/collections@^1.1.3",
    "@std/crypto": "jsr:@std/crypto@^1.0.5",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@std/http": "jsr:@std/http@^1.0.22",
    "@std/http/unstable-route": "jsr:@std/http/unstable-route",
    "preact": "npm:preact@^10.28.0",
    "preact-render-to-string": "npm:preact-render-to-string@^6.6.4"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": false
  },
  "deploy": {
    "org": "jhnadal",
    "app": "nadal-link-shortener"
  }
}
